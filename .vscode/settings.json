    "third_party/wezterm-types/types/objects/pane-information.lua",
    "third_party/wezterm-types/types/objects/pane.lua",
    "third_party/wezterm-types/types/objects/spawn-command.lua",
    "third_party/wezterm-types/types/objects/ssh-domain.lua",
    "third_party/wezterm-types/types/objects/tab-information.lua",
    "third_party/wezterm-types/types/objects/time.lua",
    "third_party/wezterm-types/types/objects/tls-domain-client.lua",
