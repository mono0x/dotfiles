{
  "[markdown]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80
  },
  "dotfiles.installCommand": "~/.local/share/chezmoi/install.sh",
  "dotfiles.repository": "mono0x/dotfiles",
  "dotfiles.targetPath": "~/.local/share/chezmoi",
  "editor.accessibilitySupport": "off",
  "editor.copyWithSyntaxHighlighting": false,
  "editor.cursorStyle": "line",
  "editor.detectIndentation": false,
  "editor.fontFamily": "HackGen Console NF, monospace",
  "editor.fontSize": 15,
  "editor.formatOnSave": true,
  "editor.inlineSuggest.enabled": true,
  "editor.lineHeight": 16,
  "editor.lineNumbers": "on",
  "editor.minimap.enabled": false,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "files.associations": {
    "*.mdx": "markdown"
  },
  "files.autoSave": "onFocusChange",
  "files.autoSaveWorkspaceFilesOnly": true,
  "files.insertFinalNewline": true,
  "redhat.telemetry.enabled": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.minimumContrastRatio": 1,
  "vim.handleKeys": {
    "<C-p>": false
  },
  "vim.hlsearch": true,
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    },
    {
      "before": ["g", "j"],
      "after": ["j"]
    },
    {
      "before": ["g", "k"],
      "after": ["k"]
    },
    {
      "before": ["<leader>", "g", "b"],
      "commands": [{ "command": "gitlens.toggleFileBlame" }]
    },
    {
      "before": ["<leader>", "g", "h"],
      "commands": [{ "command": "gitlens.openFileOnRemote" }]
    }
  ],
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["j"],
      "after": ["g", "j"]
    },
    {
      "before": ["k"],
      "after": ["g", "k"]
    },
    {
      "before": ["g", "j"],
      "after": ["j"]
    },
    {
      "before": ["g", "k"],
      "after": ["k"]
    },
    {
      "before": ["<leader>", "g", "h"],
      "commands": [{ "command": "gitlens.openFileOnRemote" }]
    }
  ],
  "vim.visualstar": true,
  "window.newWindowDimensions": "inherit",
  "workbench.colorTheme": "Solarized Light",
  "workbench.editor.enablePreview": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.enableExperiments": false,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "none",
  "workbench.tree.indent": 16
}
