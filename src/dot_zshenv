# .zshenv
# vim: foldmethod=marker

# XDG {{{
export XDG_CONFIG_HOME="$HOME/.config"
export XDG_DATA_HOME="$HOME/.local/share"
export XDG_STATE_HOME="$HOME/.local/state"
export XDG_CACHE_HOME="$HOME/.cache"
# }}}

source "$XDG_CONFIG_HOME/zsh/zshenv.d/profiler.zsh"
source "$XDG_CONFIG_HOME/zsh/zshenv.d/env.zsh"
source "$XDG_CONFIG_HOME/zsh/zshenv.d/homebrew.zsh"
source "$XDG_CONFIG_HOME/zsh/zshenv.d/rtx.zsh"
