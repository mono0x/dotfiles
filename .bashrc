#!/bin/bash

alias ll='ls -l'
alias la='ls -A'

alias g='git'
alias ga='git add'
alias gbr='git br'
alias gci='git ci'
alias gco='git co'
alias gd='git di'
alias gf='git fetch --all'
alias gg='git grep -H --heading -I --line-number --break --show-function'
alias gl='git log'
alias gme='git me'
alias gpr='git pull --rebase'
alias gs='git status'

source ~/.bashrc.default
