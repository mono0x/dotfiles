{
  "deno.enable": true,
  "deno.importMap": "deno.jsonc",
  "deno.lint": true,
  "files.associations": {
    "**/src/dot_config/git/conf.d/*.*": "ini",
    "**/src/dot_config/git/config": "ini",
    "**/src/dot_config/git/ignore": "ignore",
    "*.toml.tmpl": "toml",
    "*.zsh.tmpl": "shellscript",
    "dot_zprofile": "shellscript",
    "dot_zshenv": "shellscript",
    "dot_zshrc.tmpl": "shellscript"
  },
  "[json][jsonc][typescript][yaml]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "files.exclude": {
    "third_party": true
  },
  "yaml.schemas": {
    // https://github.com/github/vscode-github-actions/issues/195#issuecomment-1570218818
    "https://json.schemastore.org/github-workflow.json": [
      "*.github/workflows/*.yaml",
      "*.github/workflows/*.yml"
    ],
    "https://json.schemastore.org/github-action.json": [
      "action.yaml",
      "action.yml"
    ],
    // https://github.com/pqrs-org/Karabiner-Elements/issues/1918#issuecomment-1768131792
    "https://raw.githubusercontent.com/chrisgrieser/.config/refs/heads/main/karabiner/assets/complex_modifications/schema/karabiner-mod-schema.json": [
      "karabiner/karabiner.json"
    ],
    "https://raw.githubusercontent.com/goss-org/goss/master/docs/schema.yaml": [
      "goss.yaml",
      "test/goss/*.yaml"
    ]
  },
  "Lua.diagnostics.libraryFiles": "Enable",
  "Lua.workspace.library": [
    "third_party/neodev.nvim/types/stable/alias.lua",
    "third_party/neodev.nvim/types/stable/api.lua",
    "third_party/neodev.nvim/types/stable/cmd.lua",
    "third_party/neodev.nvim/types/stable/lpeg.lua",
    "third_party/neodev.nvim/types/stable/lua.lua",
    "third_party/neodev.nvim/types/stable/options.1.lua",
    "third_party/neodev.nvim/types/stable/options.2.lua",
    "third_party/neodev.nvim/types/stable/options.3.lua",
    "third_party/neodev.nvim/types/stable/options.lua",
    "third_party/neodev.nvim/types/stable/uv.lua",
    "third_party/neodev.nvim/types/stable/vim.fn.1.lua",
    "third_party/neodev.nvim/types/stable/vim.fn.lua",
    "third_party/neodev.nvim/types/stable/vim.lua",
    "third_party/wezterm-types/types/config/audible-bell.lua",
    "third_party/wezterm-types/types/config/background.lua",
    "third_party/wezterm-types/types/config/daemon-options.lua",
    "third_party/wezterm-types/types/config/init.lua",
    "third_party/wezterm-types/types/config/quoted-dropped-files.lua",
    "third_party/wezterm-types/types/config/ui-key-cap-rendering.lua",
    "third_party/wezterm-types/types/config/visual-bell.lua",
    "third_party/wezterm-types/types/config/win32-system-backdrop.lua",
    "third_party/wezterm-types/types/config/window-padding.lua",
    "third_party/wezterm-types/types/enum/copy-mode-assignment.lua",
    "third_party/wezterm-types/types/enum/key-assignment.lua",
    "third_party/wezterm-types/types/events/gui.lua",
    "third_party/wezterm-types/types/events/multiplexer.lua",
    "third_party/wezterm-types/types/events/window.lua",
    "third_party/wezterm-types/types/init.lua",
    "third_party/wezterm-types/types/objects/color.lua",
    "third_party/wezterm-types/types/objects/exec-domain.lua",
